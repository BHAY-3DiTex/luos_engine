{
    "name": "Luos",
    "keywords": "robus,network,microservice,luos,operating system,os,embedded,communication,service,ST",
    "description": "Luos turns your embedded system into services like microservices architecture does it in software.",
    "version": "2.1.1",
    "authors": {
        "name": "Luos",
        "url": "https://luos.io"
    },
    "build": {
        "flags": [
            "-I inc",
            "-I Robus/inc",
            "-I OD",
            "-I Profiles/Core",
            "-I Profiles/State",
            "-I Profiles/Motor",
            "-I Profiles/Servo_motor",
            "-I Profiles/Voltage",
            "-I Bootloader",
            "-I test/resources/Unity",
            "-I test/resources/Mock",
            "-I test/resources/Scenarios"
        ],
        "srcFilter": [
            "+<*.c>",
            "+<../Robus/src/*.c>",
            "+<../Profiles/Core/*.c>",
            "+<../Profiles/State/*.c>",
            "+<../Profiles/Motor/*.c>",
            "+<../Profiles/Servo_motor/*.c>",
            "+<../Profiles/Voltage/*.c>",
            "+<../Bootloader/*.c>",
            "+<../test/resources/Unity/*.c>",			
            "+<../test/resources/Mock/*.c>",
            "+<../test/resources/Scenarios/*.c>"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Luos-io/luos"
    }
}